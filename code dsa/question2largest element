class Solution {
public:
    int dominantIndex(vector<int>& nums) {
        int n= nums.size();
        int maxNumber=0;
        for(int i=0;i<n;i++){
            if(nums[i]>nums[maxNumber]){
                maxNumber=i;     

            }
        }
        for (int i = 0; i < n; i++) {
            if (i != maxNumber && nums[maxNumber] < 2 * nums[i]) {
                return -1;
            }
        }

        

    return maxNumber;   
    }
};
